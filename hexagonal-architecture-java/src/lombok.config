# To stop lombok from looking at parent directories for more configuration files
config.stopBubbling = true
# Adds an @Generated annotation to the methods generated by Lombok.
# This is necessary so that the method generated by Lombok are not analyzed by Sonar.
# The condition coverage in Sonar would otherwise be messed up.
lombok.addLombokGeneratedAnnotation = true
# Copy @Qualifier and @Value annotations from instance variables to generated constructors
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Qualifier
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Value