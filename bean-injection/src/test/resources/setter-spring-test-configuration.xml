<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="injectedObject" class="com.baeldung.spring.tutorial.InjectedObject" />
	
	<bean id="setterInjectionReceiver" class="com.baeldung.spring.tutorial.SetterInjectionReceiver">
		<property name="internalSetterInjectedObject" ref="injectedObject" />
	</bean>
	
	<bean id="setterInjectionReceiverTest" class="com.baeldung.spring.tutorial.SetterInjectionReceiverTest" >
		<property name="setterInjectionReceiver" ref="setterInjectionReceiver" />
	</bean>
</beans>