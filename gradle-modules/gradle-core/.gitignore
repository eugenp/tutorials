!gradle-wrapper.jar
# IntelliJ IDEA-specific files
.idea/
*.iws
*.iml
*.ipr

# Target folder for build output
/out/

# Gradle-specific files
/.gradle/
/build/
/buildSrc/build/
/buildSrc/.gradle/

