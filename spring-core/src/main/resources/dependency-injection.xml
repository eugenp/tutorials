<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="testDependency" class="com.baeldung.dependency.TestDependency" />

	<bean id="constructorDependencyInjectionXML" class="com.baeldung.dependency.ConstructorDependencyInjection">
		<!-- constructor injection -->
		<constructor-arg>
			<ref bean="testDependency" />
		</constructor-arg>
	</bean>

	<bean id="constructorDependencyInjectionAnnotation" class="com.baeldung.dependency.ConstructorDependencyInjection">
		<!-- Constructor dependency injection will happen through annotation in ConstructorDependencyInjection 
			class -->
	</bean>

	<bean id="setterDependencyInjectionXML" class="com.baeldung.dependency.SetterDependencyInjection">
		<!-- setter injection -->
		<property name="dependency" ref="testDependency" />
	</bean>

	<bean id="setterDependencyInjectionAnnotation" class="com.baeldung.dependency.SetterDependencyInjection">
		<!-- setter injection will happen through annotation in SetterDependencyInjection 
			class -->
	</bean>

</beans>
