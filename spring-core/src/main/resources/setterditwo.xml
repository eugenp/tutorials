<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="music" class="com.baeldung.setterditwo.domain.Music">
        <property name="artist" ref="artist" />
        <property name="song" ref="song" />
    </bean>

    <bean id="artist" class="com.baeldung.setterditwo.domain.Artist">
        <property name="name" value="Yo-Yo Ma" />
        <property name="age" value="61" />
    </bean>

    <bean id="song" class="com.baeldung.setterditwo.domain.Song">
        <property name="name" value="Joy to the World" />
    </bean>

</beans>
