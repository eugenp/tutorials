<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<bean id="search" class="com.baeldung.evaluation.Search" />
	<bean id="map" class="com.baeldung.evaluation.Map" />
	<bean id="park" class="com.baeldung.evaluation.Park">
	    <constructor-arg type="com.baeldung.evaluation.Map">
            <ref bean="map" />
        </constructor-arg>
        <property name="searcher">
            <ref bean="search" />
        </property>
    </bean>
</beans>