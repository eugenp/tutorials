<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="categoryImporter" class="com.baeldung.dependency_injection_types.runners.CategoryImporter">
        <constructor-arg ref="categoryRepository"/>
    </bean>

    <bean id="categoryRepository" class="com.baeldung.dependency_injection_types.repositories.CategoryRepository">
    </bean>

</beans>
