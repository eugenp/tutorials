<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


    <bean id="city" class="com.baeldung.example.construct.ProjectService">
        <constructor-arg name="projectDao" ref="projectDao"/>
    </bean>
    <bean id="projectDao" class="com.baeldung.example.construct.ProjectDao"/>


    <bean id="project" class="com.baeldung.example.construct.Project">

        <constructor-arg index="1">
            <value>1</value>
        </constructor-arg>

        <constructor-arg index="0">
            <value>Project Name</value>
        </constructor-arg>

    </bean>

</beans>
