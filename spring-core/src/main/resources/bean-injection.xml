<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="hospitalBean" class="com.baeldung.beaninjectiontypes.setter.Hospital">
        <property name="patient" ref="patientBean"/>
    </bean>

    <bean id="patientBean" class="com.baeldung.beaninjectiontypes.setter.Patient">
        <property name="firstName" value="John"/>
    </bean>

    <bean id="libraryBean" class="com.baeldung.beaninjectiontypes.constructor.Library">
        <constructor-arg name="manager" ref="managerBean"/>
    </bean>

    <bean id="managerBean" class="com.baeldung.beaninjectiontypes.constructor.Manager">
        <property name="name" value="Stacy"/>
    </bean>
</beans>