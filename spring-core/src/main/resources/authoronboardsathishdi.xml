<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="musicMaker" class="com.baeldung.authoronboardsathish.domain.MusicMaker">
        <constructor-arg ref="violinMusicMaker"/>
    </bean>

    <bean id="violinMusicMaker" class="com.baeldung.authoronboardsathish.domain.ViolinMusicMaker"/>

    <bean id="pianoMusicMaker" class="com.baeldung.authoronboardsathish.domain.PianoMusicMaker"/>

</beans>
