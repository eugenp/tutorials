<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"

	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
          http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
">
	<bean name="dao" class="com.baeldung.injectiontypes.dao.StudentDAO" />
	<bean name="serviceInjectDependencyByConstrucot" class="com.baeldung.injectiontypes.xmlconfig.service.StudentService">
		<constructor-arg>
				<bean class="com.baeldung.injectiontypes.dao.StudentDAO" />
			
		</constructor-arg>
	</bean>
	<bean name="serviceInjectDependencyBySetter" class="com.baeldung.injectiontypes.xmlconfig.service.StudentService">
		<property name="dao">
			<bean class="com.baeldung.injectiontypes.dao.StudentDAO" />
		</property>
	</bean>

</beans>
